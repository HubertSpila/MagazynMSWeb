@addTagHelper*, Microsoft.AspNetCore.Mvc.TagHelpers
@using WarehouseManagmentWEB.Controllers
@using WarehouseManagmentWEB.Tools
@using WarehouseManagmentWEB.Tools.Api;
@model IndexModel
@{
    ViewBag.Title = "Login";
}
<head>
    <title>@ViewBag.Title</title>
</head>
<body>
    <div class="text-center">

        @*<li><a asp-controller="App" asp-action="Index">Odnośnik do controllera</a> </li>
        <button><a asp-controller="App" asp-action="Index">Odnośnik do strony</a> </button>*@

        @using (Html.BeginForm("Index", "App", FormMethod.Post))
        {
            <form method="post">
                <input type="text" name="login" />
                <input type="password" name="password" />
                <input type="submit" asp-asp-page-handler="Submit">
                <hr />
            </form>
        }
        <p>@Model?.IsAuthorized</p>
        <br>

    </div>
</body>
